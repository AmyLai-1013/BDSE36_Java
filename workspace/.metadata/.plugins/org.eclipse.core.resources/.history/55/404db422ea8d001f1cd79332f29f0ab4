package com.siang.HW02;

public class Expression {

	public static void main(String[] args) {
		int sum1 = 0, sum2 = 0,sum;
		for(int i = 1; i <= 50; i += 2) {
			int value = i * i;
			sum1 += value;
		}
		for(int i = 0; i <= 50; i += 2) {
			int value = i * i;
			sum2 += value;
		}
		sum = sum1 - sum2;
		System.out.println(sum);
	}

}
