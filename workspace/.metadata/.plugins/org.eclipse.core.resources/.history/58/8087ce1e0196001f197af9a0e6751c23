package com.siang.HW03;

public class MaxMin {

	public static void main(String[] args) {
		int[] data = { 1, 2, 87, 144, 5, -225, -4, 6, 98, 4 };
		int min = data[0], max = data[0];
		for (int i = 0; i < data.length; i++) {
			if (data[i] > max) {
				max = data[i];
			} 
			if (data[i] < min) {
				min = data[i];
			}
		}
		System.out.println("Max = " + max);
		System.out.println("Min = " + min);

	}

}
